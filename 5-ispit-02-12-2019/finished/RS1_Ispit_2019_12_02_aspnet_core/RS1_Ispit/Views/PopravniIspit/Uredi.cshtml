
@{
    ViewBag.Title = "Index";


}
@model RS1_Ispit_asp.net_core.ViewModels.PopravniIspitUrediVM

<form>
    <div class="form-group">
        <label>Predmet</label>
        <input class="form-control" asp-for="Predmet" readonly/>

    </div>
    <div class="form-group">
        <label>Datum ispita</label>
        <input class="form-control" asp-for="Datum" readonly />
    </div>
    <div class="form-group">
        <label>Skola</label>
        <input class="form-control" asp-for="Skola" readonly />
    </div>
    <div class="form-group">
        <label>Skolska godina</label>
        <input class="form-control" asp-for="SkolskaGodina" readonly />
    </div>
    <div class="form-group">
        <label>Odjeljenje</label>
        <input class="form-control" asp-for="Odjeljenje" readonly />
    </div>
</form>

<div id="divID"></div>

<script>
    $(document).ready(function () {
        $.get("/stavke/index?ispitId=@Model.IspitID", function (data, status) {
            $("#divID").html(data);
        });
    });
</script>