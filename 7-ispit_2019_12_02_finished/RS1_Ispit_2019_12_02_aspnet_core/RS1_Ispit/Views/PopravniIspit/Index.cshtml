
@{
    ViewBag.Title = "Index";


}
@model RS1_Ispit_asp.net_core.ViewModels.PopravniIspitIndexVM
<table class="table">
    <thead>
        <tr>
            <th>Skolska godina</th>
            <th>Skola</th>
            <th>Odjeljenje</th>
            <th>Akcija</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model.rows)
        {
            <tr>
                <td>@x.SkolskaGodina</td>
                <td>@x.Skola</td>
                <td>@x.Odjeljenje</td>
                <td><a class="btn btn-primary" asp-action="Odaberi" asp-route-odjeljenjeId="@x.OdjeljenjeId">Odaberi</a></td>
            </tr>
        }
    </tbody>
</table>



