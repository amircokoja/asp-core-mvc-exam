
@{
    ViewBag.Title = "Index";


}
@model RS1_Ispit_asp.net_core.ViewModels.PopravniUrediVM

    <form>
        <div class="form-group">
            <label>Predmet</label>
            <input class="form-control" asp-for="Naziv" readonly />
        </div>
        <div class="form-group">
            <label>Razred</label>
            <input class="form-control" asp-for="Razred" readonly />
        </div>
        <div class="form-group">
            <label>Datum ispita</label>
            <input asp-for="Datum" class="form-control" />
        </div>
        <div class="form-group">
            <label>Škola</label>
            <input asp-for="Skola" class="form-control" />

        </div>
        <div class="form-group">
            <label>Školska godina</label>
            <input asp-for="SkolskaGodina" class="form-control" />
        </div>
    </form>

<div id="divID"></div>


<script>
    $(document).ready(function () {
        $.get("/ajaxstavke/index?popravniIspitId=@Model.PopravniIspitId", function (data, status) {
            $("#divID").html(data);
        });
    });
</script>